---
import BaseHead from '../components/BaseHead.astro';
import Layout from '../components/Layout.astro'
let title = 'Nick Henderson - Bio';
let description = 'Composer | Multi-instrumentalist | recording engineer';
let permalink = 'https://nickhendersonmusic.com/bio';
import Bio from '../components/Bio.svelte'
import Container from '../components/Container.svelte'

---
<html lang="en" class="bg-white dark:bg-black">
  <head>
    <BaseHead title={title} description={description} permalink={permalink} />
  </head>

  <Layout client:load currentPath="/bio" useContainer={true} >
    <Bio/>
  </Layout>

</html>
